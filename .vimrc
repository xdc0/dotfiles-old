syntax on
colorscheme wombat
set number
set smartindent
"set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
"set textwidth=79
filetype plugin on
let NERDTreeWinPos='right'
"let NERDTreeQuitOnOpen=1
map BN :bn!<CR>
let Tlist_Auto_Open=1
map BP :bp!<CR>
set gfn=Dina
set guioptions-=T
set guioptions+=rlbRLB
set guioptions-=rlbRLB
